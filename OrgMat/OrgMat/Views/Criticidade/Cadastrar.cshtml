@model OrgMat.Models.CriticidadeModel
@{
    ViewData["Title"] = "Cadastrar";
}

<div class="pagetitle">
    <h1 class="display-4"> Cadastrar: Criticidade</h1>
</div>

<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                <form method="post" action="Create">
                    <label for="inputText" class="col-sm col-form-label">Criticidade:</label>
                    <input type="text" class="form-control" id="criticidade" name="criticidade" asp-for="criticidade" />
                    <br />
                    <button type="submit" class="btn btn-primary" name="submit" value="submit">Cadastrar</button>
                </form>
            </div>
        </div>
    </div>
</div>

